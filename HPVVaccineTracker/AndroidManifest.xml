        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:name="com.irespond.hpvvaccinetracker.HPVVaccineTrackerApp">
        <activity android:configChanges="orientation" android:screenOrientation="portrait" android:name="com.irespond.hpvvaccinetracker.LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.irespond.hpvvaccinetracker.ProviderLoginActivity" android:label="@string/app_name" android:screenOrientation="portrait" android:configChanges="orientation"></activity>
        <activity android:name="com.irespond.hpvvaccinetracker.EnrollmentActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name"> </activity>
        
        <!--
        <activity
            android:name="com.irespond.hpvvaccinetracker.HPVVaccineTracking"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        -->
        <activity android:name="com.irespond.biometrics.client.IrespondActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name">
        </activity>
