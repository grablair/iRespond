        <activity android:name="com.irespond.hpvvaccinetracker.PatientFoundActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name"></activity>
        <activity android:name="com.irespond.hpvvaccinetracker.PatientNotFoundActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name"></activity>
        <activity android:name="com.irespond.hpvvaccinetracker.GiveDoseActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name"></activity>
        <activity android:name="com.irespond.hpvvaccinetracker.ReturnDateActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name"></activity>
        <activity android:name="com.irespond.hpvvaccinetracker.SeriesCompleteActivity" android:screenOrientation="portrait" android:configChanges="orientation" android:label="@string/app_name"></activity>
